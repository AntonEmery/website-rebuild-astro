---
import Layout from '../layouts/Layout.astro';
import HeaderSection from '../layouts/HeaderSection.astro';
import MainContentSection from '../layouts/MainContentSection.astro';
const title = 'Gear';
const sectionClassName = 'gear';
---

<Layout
  title='Anton Emery | Gear'
  description='The instruments of Anton Emery, celtic guitarist'
  keywords='celtic, guitar, acoustic, sheet musc, tabs'
>
  <HeaderSection
    sectionClassName={sectionClassName}
    title={title}
    imagePath='/src/assets/guitar-banjo.jpg'
    altText='guitar and banjo leaning on the couch'
  />
  <MainContentSection title={title} />
</Layout>
